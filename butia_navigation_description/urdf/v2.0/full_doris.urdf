  <robot name="doris">
<!--Links declaration-->
  <link name="base_link"/>
  <link name="hokuyo_link"/>
  <link name="sick_link"/>
  <link name="torso_link"/>
  <link name="arm_link"/>
  <link name="head_link"/>
<!--Joints declaration-->
  <joint name="hokuyo_base" type="continuous">
	<parent link="base_link"/>
	<child link="hokuyo_link"/>
  </joint>
  <joint name="torso_base" type="continuous">
	<parent link="base_link"/>
	<child link="torso_link"/>
  </joint>
  <joint name="sick_torso">
	<parent link="torso_link"/>
	<child link="sick_link"/>
  </joint>
  <joint name="arm_torso">
	<parent link="torso_link"/>
	<child link="arm_link"/>
  </joint>
  <joint name="head_torso">
	<parent link="torso_link"/>
	<child link="head_link"/>
  </joint>

